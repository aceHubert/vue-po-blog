{
  /** back-end server port */
  "server_port": 5010,
  /** The name of the database */
  "db_name": "po-blog",
  /** database username */
  "db_user": "username",
  /** database password */
  "db_password": "password",
  /** hostname */
  "db_host": "localhost",
  /** port */
  "db_port": 3306,
  /** Database Charset to use in creating database tables. */
  "db_charset": "utf8",
  /** The Database type. */
  "db_dialect": "mysql",
  /** The Database Collate type. Don't change this if in doubt. */
  "db_collate": "",
  /** table prefix */
  "table_prefix": "po_",
  /** jwt screct */
  "jwt_screct": "e83a8c67-3df0-4feb-8d90-09a354b23b1f",
  /** jwt algorithm  
      "HS256" | "HS384" | "HS512" |
      "RS256" | "RS384" | "RS512" |
      "ES256" | "ES384" | "ES512" |
      "PS256" | "PS384" | "PS512" |
      "none" */
  "jwt_algorithm": "HS256",
  /** jwt expiresIn https://github.com/vercel/ms */
  "jwt_expiresIn": "30m",
  "jwt_refresh_token_expiresIn": "15d",
  /** Cache TTL */
  "cache_ttl": 5,
  /** Cache Max */
  "cache_max": 1000,
  /** disk or memory , https://github.com/expressjs/multer#memorystorage*/
  "file_storage": "disk",
  /** diskStorage destination, default: os.tmpdir() */
  "file_dest": null,
  /** file limits, https://github.com/expressjs/multer#limits */
  "file_limits":{
    // 2M, in bytes
    "fileSize":2080800
  }
}
